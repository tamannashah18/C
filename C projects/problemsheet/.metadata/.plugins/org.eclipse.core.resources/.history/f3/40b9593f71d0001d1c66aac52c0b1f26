#include<stdio.h>
#include<stdlib.h>
int main(int argc,char *argv[2])
{
	int a,i,cnt=0;
	a=atoi(argv[1]);
	for(i=0;i<=a;i++)
	{
		if(a%i==0)
			cnt++;
	}
	if(cnt==2)
	{
		printf("%d IS A PRIME NO.",a);
	}
	else if(a==0 || a==1)
		{
			printf("%d IS A SPECIAL NO.",a);
		}
	else
	{
		printf("%d IS A COMPOSITE NO.",a);
	}
}
